<html>
  <head>
    <script src="/asset/bundle.js"></script>
    <link rel="stylesheet" href="/asset/bundle.css" />
    <title>Sign Up</title>
  </head>
  <body>
    <div class="signup">
      <div class="signup-field-box">
        <h1>Sign Up</h1>
        <img id="linedecor" src="/asset/signup-linedecor1.png" />
        <input id="email-input" type="text" placeholder="Email" />
        <input id="username-input" type="text" placeholder="Username" />
        <input id="pwd-input" type="password" placeholder="Password" />
        <input
          id="pwd-confirm-input"
          type="password-confirm"
          placeholder="Confirm Password"
        />
        <input id="zip-input" type="zipcode" placeholder="Zip Code" />
        <button>
          <img src="/asset/signup-arrow.png" />
        </button>
      </div>
    </div>
  </body>
</html>
